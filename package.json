{
  "name": "react-server-demo",
  "version": "1.0.0",
  "description": "react server render demo",
  "main": "index.js",
  "scripts": {
    "1-static": "webpack-dev-server --progress --colors --inline --config ./1_simple_server_render/webpack/static",
    "1-dev": "rimraf ./1_simple_server_render/dist/* && cross-env NODE_ENV=test nodemon ./1_simple_server_render/server.js --watch server --watch build",
    "1-build": "rimraf ./1_simple_server_render/dist/* && cross-env NODE_ENV=production webpack --config ./1_simple_server_render/webpack/server-build",
    "1-run": "cross-env NODE_ENV=production node ./1_simple_server_render/dist/server/main.js",
    "2-static": "webpack-dev-server --progress --colors --inline --config ./2_route_redux_render/webpack/static",
    "2-dev": "rimraf ./2_route_redux_render/dist/* && cross-env NODE_ENV=test nodemon ./2_route_redux_render/server.js --watch server --watch build",
    "2-build": "rimraf ./2_route_redux_render/dist/* && cross-env NODE_ENV=production webpack --config ./2_route_redux_render/webpack/server-build",
    "2-run": "cross-env NODE_ENV=production node ./2_route_redux_render/dist/server/main.js",
    "3-static": "webpack-dev-server --progress --colors --inline --config ./3_compress_ensure_render/webpack/static",
    "3-dev": "rimraf ./3_compress_ensure_render/dist/* && cross-env NODE_ENV=test nodemon ./3_compress_ensure_render/server.js --watch server --watch build",
    "3-build": "cross-env NODE_ENV=production webpack --config ./3_compress_ensure_render/webpack/server-build",
    "3-run": "cross-env NODE_ENV=production node ./3_compress_ensure_render/dist/server/server.js",
    "3-start": "pm2 start ./3_compress_ensure_render/pm2.config.js",
    "3-stop": "pm2 stop ./3_compress_ensure_render/pm2.config.js & pm2 delete ./3_compress_ensure_render/pm2.config.js & pm2 kill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chkui/react-server-demo.git"
  },
  "keywords": [
    "react"
  ],
  "author": "chkui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chkui/react-server-demo/issues"
  },
  "homepage": "https://github.com/chkui/react-server-demo#readme",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.10.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "asset-require-hook": "^1.1.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-modules-require-hook": "^4.0.5",
    "classnames": "^2.2.5",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "ejs": "^2.5.5",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "node-fetch": "^1.7.1",
    "node-noop": "^1.0.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compose": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.0",
    "koa-session": "^3.4.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.0",
    "koa-webpack-dev-middleware": "^1.3.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "pm2": "^2.6.1",
